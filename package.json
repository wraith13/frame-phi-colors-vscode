{
	"name": "frame-phi-colors",
	"displayName": "Frame Phi Colors",
	"description": "This extension colors VS Code's frame.",
	"icon": "images/phihex.128.png",
	"version": "0.0.1",
	"publisher": "wraith13",
	"license": "SEE LICENSE IN LICENSE_1_0.txt",
	"engines": {
		"vscode": "^1.38.0"
	},
	"bugs": {
		"url": "https://github.com/wraith13/frame-phi-colors-vscode/issues"
	},
	"homepage": "https://github.com/wraith13/frame-phi-colors-vscode",
	"repository": {
		"type": "git",
		"url": "https://github.com/wraith13/frame-phi-colors-vscode.git"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/source/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		],
		"configuration":{
			"title": "Frame Phi Colors",
			"properties": {
				"framePhiColors.applyScope": {
					"type": "string",
					"default": "has .vscode or .git",
					"enum": [
						"none",
						"has .vscode",
						"has .vscode or .git",
						"any"
					],
					"description": "%framePhiColors.applyScope.description%"
				},
				"framePhiColors.baseColor": {
					"type": "string",
					"default": "#5679C9",
					"description": "%framePhiColors.baseColor.description%"
				},
				"framePhiColors.titleBarColorSource": {
					"type": "string",
					"default": "hostname",
					"enum": [
						"none",
						"hostname",
						"workspace",
						"workspace-folder",
						"document-fullpath",
						"document",
						"file-type"
					],
					"description": "%framePhiColors.titleBarColorSource.description%"
				},
				"framePhiColors.activityBarColorSource": {
					"type": "string",
					"default": "workspace",
					"enum": [
						"none",
						"hostname",
						"workspace",
						"workspace-folder",
						"document-fullpath",
						"document",
						"file-type"
					],
					"description": "%framePhiColors.activityBarColorSource.description%"
				},
				"framePhiColors.statusBarColorSource": {
					"type": "string",
					"default": "document",
					"enum": [
						"none",
						"hostname",
						"workspace",
						"workspace-folder",
						"document-fullpath",
						"document",
						"file-type"
					],
					"description": "%framePhiColors.statusBarColorSource.description%"
				},
				"framePhiColors.statusBarDebuggingColorSource": {
					"type": "string",
					"default": "document",
					"enum": [
						"none",
						"hostname",
						"workspace",
						"workspace-folder",
						"document-fullpath",
						"document",
						"file-type"
					],
					"description": "%framePhiColors.statusBarDebuggingColorSource.description%"
				},
				"framePhiColors.titleBarColorMode": {
					"type": "string",
					"default": "nega-dark",
					"enum": [
						"posi-light",
						"posi-dark",
						"nega-light",
						"nega-dark"
					],
					"description": "%framePhiColors.titleBarColorMode.description%"
				},
				"framePhiColors.activityBarColorMode": {
					"type": "string",
					"default": "nega-dark",
					"enum": [
						"posi-light",
						"posi-dark",
						"nega-light",
						"nega-dark"
					],
					"description": "%framePhiColors.activityBarColorMode.description%"
				},
				"framePhiColors.statusBarColorMode": {
					"type": "string",
					"default": "posi-light",
					"enum": [
						"posi-light",
						"posi-dark",
						"nega-light",
						"nega-dark"
					],
					"description": "%framePhiColors.statusBarColorMode.description%"
				},
				"framePhiColors.statusBarDebuggingColorMode": {
					"type": "string",
					"default": "posi-dark",
					"enum": [
						"posi-light",
						"posi-dark",
						"nega-light",
						"nega-dark"
					],
					"description": "%framePhiColors.statusBarDebuggingColorMode.description%"
				},
				"framePhiColors.statusBarNoFolderColorMode": {
					"type": "string",
					"default": "nega-dark",
					"enum": [
						"posi-light",
						"posi-dark",
						"nega-light",
						"nega-dark"
					],
					"description": "%framePhiColors.statusBarNoFolderColorMode.description%"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"phi-colors": "^1.0.0"
	}
}
